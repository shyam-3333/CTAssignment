        function moveBack(){
            
            if ((typeof sforce != 'undefined') && sforce && (!!sforce.one)){ 
                
                sforce.one.navigateToURL("/apex/DummyTaneiraProd");
            }
            else{
                console.log('**');
                window.open('/apex/DummyTaneiraProd','_self');
            }
        }
        function openRecordView(proId) {
            if ((typeof sforce != 'undefined') && sforce && (!!sforce.one)) {
                sforce.one.navigateToSObject(proId);
            } else {
                window.open('/' + proId);
            }
        }
