                                                        <apex:inputfield value="{!wrpImgCls.prod.Body_Des_Material__c}"  /></td>
                                                    <td role="gridcell" data-label="BODY DES DENSITY ">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Body_Des_Density__c}"  /></td>
                                                    <td role="gridcell" data-label="BORDER PATTERN">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Border_Pattern__c}"  /></td>
                                                    <td role="gridcell" data-label="BORDER DES ELEMENT ">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Border_Des_Element__c}"  /></td>
                                                    <td role="gridcell" data-label="BORDER DES MATERIAL ">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Border_Des_Material__c}"  /></td>
                                                    <td role="gridcell" data-label="BORDER SIZE ">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Border_Size__c}"  /></td>
                                                    <td role="gridcell" data-label="PALLU PATTERN">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Pallu_Pattern__c}"  /></td>
                                                    <td role="gridcell" data-label="PALLU DESIGN">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Pallu_Design__c}"  /></td>
                                                    <td role="gridcell" data-label="PALLU DES MATERIAL">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Pallu_Des_Material__c}"  /></td>
                                                    <td role="gridcell" data-label="PALLU DES DENSITY ">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Pallu_Des_Density__c}"  /></td>
                                                    <td role="gridcell" data-label="PALLU SIZE">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Pallu_Size__c}"  /></td>
                                                    <td role="gridcell" data-label="WAREHOUSE STOCK">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Warehouse_Stock__c}"  /></td>
                                                    <td role="gridcell" data-label="SOLD STOCK">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Sold_Stock__c}"  /></td>
                                                    <td role="gridcell" data-label="STORE STOCK ">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Store_Stock__c}"  /></td>
                                                    <td role="gridcell" data-label="ORDER ID">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Order_ID__c}"  /></td>
                                                    <td role="gridcell" data-label="COMMENTS">
                                                        <apex:inputfield value="{!wrpImgCls.prod.Remarks__c}"  /></td>
                                                    <td role="gridcell" data-label="Created By">
                                                        <apex:outputField value="{!wrpImgCls.prod.CreatedBy.Name}" /></td>
                                                    <td>  
                                                        <apex:commandButton value="Remove" action="{!deleteAddedRow}" reRender="frm" > 
                                                            <apex:param value="{!wrpImgCls.counter}" assignTo="{!index}" name="index" /> 
                                                        </apex:commandButton>
                                                        
                                                    </td>
                                                    
                                                </tr>
                                            </apex:repeat>
                                        </tbody>          
                                        
                                    </table>
                                </apex:outputPanel>
                            </apex:outputPanel> 
                            <apex:outputPanel rendered="{!showPopUp}" id="popup" layout="none">
                                <section style="margin-top:50px" role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                                    <div class="slds-modal__container" style="height:80% !important">
                                        <!-- Modal/Popup Box Header Starts here-->
                                        <header class="slds-modal__header">
                                            <h2>
                                                Upload Attachments
                                            </h2>
                                        </header>
                                        <!--Modal/Popup Box Body Starts here-->
                                        <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1" style="height:100%">
                                            <apex:pageBlock >
                                                <apex:variable value="{!1}" var="attachmentSerialNo"/>
                                                <apex:pageBlockSection columns="3">
                                                    <apex:pageBlockSectionItem >Sl.No</apex:pageBlockSectionItem>
                                                    <apex:pageBlockSectionItem >Image</apex:pageBlockSectionItem>
                                                    <apex:pageBlockSectionItem >Document</apex:pageBlockSectionItem>
                                                    <apex:repeat value="{!popupAttachmentList}" var="item">
                                                        <apex:pageBlockSectionItem >{!attachmentSerialNo}</apex:pageBlockSectionItem>
                                                        <apex:pageBlockSectionItem ><apex:inputFile value="{!item.imgRec.Body}" contentType="{!item.imgRec.contentType}" fileName="{!item.imgRec.Name}" alt="None" ></apex:inputFile></apex:pageBlockSectionItem>
                                                        <apex:pageBlockSectionItem ><apex:inputFile value="{!item.docRec.Body}" contentType="{!item.docRec.contentType}" fileName="{!item.docRec.Name}" alt="None" ></apex:inputFile>
                                                            <apex:variable value="{!attachmentSerialNo+1}" var="attachmentSerialNo"/></apex:pageBlockSectionItem>
                                                    </apex:repeat>
                                                    
                                                </apex:pageBlockSection>
                                            </apex:pageBlock>
                                        </div>
                                        <apex:outputPanel id="preview"  rendered="{!showPreview}" >
                                            <div   style="height:100%">
                                                <apex:pageBlock title="Previewed Items">
                                                    <apex:variable value="{!1}" var="SerialNo"/>
                                                    <apex:pageBlockSection columns="3">
                                                        <apex:pageBlockSectionItem >Sl.No</apex:pageBlockSectionItem>
                                                        <apex:pageBlockSectionItem >Image</apex:pageBlockSectionItem>
                                                        <apex:pageBlockSectionItem >Document</apex:pageBlockSectionItem>
                                                        <apex:repeat value="{!popupAttachmentList}" var="item">
                                                            <apex:pageBlockSectionItem >{!SerialNo}</apex:pageBlockSectionItem>
                                                            <apex:pageBlockSectionItem >
                                                                <apex:repeat value="{!attachImgList}" var="img" >
                                                                    {!img.Name}
                                                                </apex:repeat>
                                                            </apex:pageBlockSectionItem>
                                                            <apex:pageBlockSectionItem >
                                                                <apex:repeat value="{!attachDOcList}" var="doc" >
                                                                    {!doc.Name}
                                                                </apex:repeat>
                                                                <apex:variable value="{!SerialNo+1}" var="SerialNo"/>
                                                            </apex:pageBlockSectionItem>
                                                            
                                                        </apex:repeat>
                                                        
                                                    </apex:pageBlockSection>
                                                </apex:pageBlock>
                                            </div>
                                        </apex:outputPanel>
                                        <!--Modal/Popup Box Footer Starts here-->
                                        <footer class="slds-modal__footer">
                                            <apex:actionRegion >
                                                <apex:commandButton value="Preview" action="{!previewAttachment}"/>
                                                <apex:commandButton action="{!hidePopup}" value="Cancel" reRender="frm"/>
                                                <apex:commandButton value="Upload" action="{!uploadAttachment}"/>
                                                
                                            </apex:actionRegion>            
                                        </footer>
                                    </div>
                                </section>
                                <div class="slds-backdrop slds-backdrop_open"></div>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!showUploadPopUp}" id="UploadPopUp" layout="none">
                                <section style="margin-top:50px" role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                                    <div class="slds-modal__container" style="height:80% !important">
                                        <header class="slds-modal__header">
                                            <h2>
                                                Upload CSV
                                            </h2>
                                            
                                        </header>
                                        <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1" style="height:100%">
                                            <apex:inputFile value="{!csvFileBody}"  filename="{!csvAsString}"/>
                                        </div>
                                        <footer class="slds-modal__footer">
                                            <apex:actionRegion >
                                                <apex:commandButton action="{!hideUploadPopUp}" value="Cancel" reRender="frm"/>
                                                <apex:commandButton value="Import File" action="{!importCSVFile}"/>
                                            </apex:actionRegion>            
                                        </footer>
                                    </div>
                                </section>  
                            </apex:outputPanel>
                        </div>
                    </div>
                </div>
            </body>
        </html>
    </apex:form>
</apex:page>