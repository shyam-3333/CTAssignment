        $(document).ready(function () {
             
            var crudServiceBaseUrl = "https://demos.telerik.com/kendo-ui/service",
            dataSource = new kendo.data.DataSource({
                 serverPaging: true,
                 serverSorting: true,
                 pageSize: 10,
                transport: {
                    read: function(e) {
                       dataSource.sync();
                       e.success(jsonDa)  ;
                    },
                    scrollable: {
                            virtual: true
                        },
                        sortable: true,
                    
                    update: function(e){
                        var StringifiedData = JSON.stringify(e.data);
                        
