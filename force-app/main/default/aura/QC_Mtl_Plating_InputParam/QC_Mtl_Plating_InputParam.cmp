                                                        <tr>
                                                            <td scope="row" data-label="Select"><lightning:input disabled="{!v.isReadonly}" type="String" value="{!testvar.testName}"/></td>
                                                            <td scope="row" data-label="Select"><lightning:input disabled="{!v.isReadonly}" type="String" value="{!testvar.standardDefinedValue}" /> </td>
                                                            <td scope="row" data-label="Select"><lightning:input disabled="{!v.isReadonly}" type="String" value="{!testvar.userInput}" /></td>
                                                            <td scope="row" data-label="Select"><lightning:input disabled="{!or(v.isReadonly,testvar.minMaxDisabled )}" type="Number" value="{!testvar.minValue}" step = "0.001" /></td>
                                                            <td scope="row" data-label="Select"><lightning:input disabled="{!or(v.isReadonly,testvar.minMaxDisabled )}" type="Number" value="{!testvar.maxValue}" step = "0.001" /></td>
                                                            <td scope="row" data-label="Select">
                                                                <lightning:select name="Verdict" disabled="{!v.isReadonly}" aura:id="verdictId" value="{!testvar.testVerdict}" >
                                                                    <aura:iteration items="{!v.finalVerdict}" var="finalVerd">
                                                                        <option text="{!finalVerd.label}" value="{!finalVerd.value}" selected="{!finalVerd.selected}" ></option>
                                                                    </aura:iteration>
                                                                </lightning:select>
                                                            </td>
                                                            <td scope="row" data-label="Select">
                                                                <a onclick="{!c.deleteRow}" disabled="{!v.isReadonly}" aura:id="{!index}" id="{!index}" >
                                                                    <lightning:icon variant="error" iconName="utility:delete" class="slds-icon slds-icon_small" size="small" alternativeText="delete"/>
                                                                    <span class="slds-assistive-text">Delete Icon</span>
                                                                </a>
                                                            </td>
                                                        </tr>  
                                                    </aura:iteration>
                                                </tbody>
                                            </table>
                                        </div>
                                        
                                    </div>
                                    
                                </aura:if>
                                
                            </div>
                        </fieldset>  
                    </aura:if>
                </div>
            </form>
        </div>
    </div>
    
</aura:component>